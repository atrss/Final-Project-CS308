{
    "source": {
        "include": ["./README.md", "./js"],
        "includePattern": ".+\\.(js)$"
    },
    "opts": {
        "template": "../node_modules/docdash",
        "encoding": "utf8",
        "destination": "./docs/",
        "recurse": true
    },
    "plugins": ["plugins/markdown"],
    "recurseDepth": 10
}
